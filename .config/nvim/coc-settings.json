{
	"languageserver": {
		"efm": {
			"command": "efm-langserver",
			"args": [],
			"filetypes": [
				"vim",
				"eruby",
				"markdown"
			]
		},
		"bash": {
			"command": "bash-language-server",
			"args": [
				"start"
			],
			"filetypes": [
				"sh"
			]
		},
		"cmake": {
			"command": "cmake-language-server",
			"filetypes": [
				"cmake"
			],
			"rootPatterns": [
				"build/"
			],
			"initializationOptions": {
				"buildDirectory": "build"
			}
		},
		"digestif": {
			"command": "digestif",
			"filetypes": [
				"tex",
				"plaintex",
				"context"
			]
		},
		"racket": {
			"command": "racket",
			"args": [
				"-l",
				"racket-langserver"
			],
			"filetypes": [
				"scheme",
				"rkt"
			]
		}
	}
}
